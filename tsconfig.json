{
    "compilerOptions": {
        "target": "es2021",
        "module": "esnext",
        "lib": ["dom", "es5"],
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "downlevelIteration": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noUncheckedIndexedAccess": true
    },
    "include": [".internal", "lib", "src"]
}
